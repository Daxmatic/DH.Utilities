<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<?xml-stylesheet href="./_c74_ref.xsl" type="text/xsl"?>

<c74object name="DH.PresetManager">

	<digest>
		pattrstorage presets
	</digest>
	
	<description>
		 A pattrstorage preset manager with sequence support
	</description>
	
	<!--METADATA-->
	<metadatalist>
		<metadata name="author">Daxmatic</metadata>
		<metadata name="tag">DH.Utilities</metadata>
		<metadata name="tag">pattrstorage</metadata>
		<metadata name="tag">presets</metadata>
	</metadatalist>
	
	<!--INLETS-->
	<inletlist>
		<inlet id="0" type="symbol">
			<digest>Pattragestorage output</digest>
			<description>The output of pattrstorage goes here.</description>
		</inlet>
		<inlet id="1" type="int">
			<digest>Counter Input</digest>
			<description>Send in the output of a counter.</description>
		</inlet>
		<inlet id="2" type="int">
			<digest>Start looppoint</digest>
			<description>Set a start looppoint.</description>
		</inlet>
		<inlet id="3" type="int">
			<digest>Enable counter</digest>
			<description>send a 0 or 1 to disable or enable sequencer support</description>
		</inlet>
	</inletlist>

	<!--OUTLETS-->
	<outletlist>
		<outlet id="0" type="symbol">
			<digest>send the messages to the pattrstorage inlet</digest>
		</outlet>
	</outletlist>
	
	<!--ARGUMENTS-->
	<objarglist>

	</objarglist>
	
	<!--MESSAGES-->
	<methodlist>

	</methodlist>
	
	<!--SEEALSO-->
	<seealsolist>
		<seealso name="pattrstorage"/>
		<seealso name="autopattr"/>
	</seealsolist>
	
</c74object>